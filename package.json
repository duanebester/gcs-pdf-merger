{
  "name": "gcs-pdf-merger",
  "version": "0.1.2",
  "description": "Merge your PDFs in the Cloud!",
  "keywords": [
    "Google Cloud Storage",
    "PDF",
    "GCS",
    "Merge"
  ],
  "author": "duanebester",
  "repository": {
    "url": "https://github.com/duanebester/gcs-pdf-merger"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "mocha --require ts-node/register 'src/**/*.test.ts'",
    "test:cov": "nyc mocha --require source-map-support/register --require ts-node/register 'src/**/*.test.ts'"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.test.ts",
      "coverage",
      "node_modules"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "fs-extra": "^10.0.0",
    "pdf-merger-js": "^3.2.0"
  },
  "devDependencies": {
    "@google-cloud/storage": "^5.11.0",
    "@types/chai": "^4.2.21",
    "@types/fs-extra": "^9.0.12",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.6",
    "@types/sinon": "^10.0.2",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
